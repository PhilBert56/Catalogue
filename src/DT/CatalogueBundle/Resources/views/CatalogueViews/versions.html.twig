{% extends 'base.html.twig' %}

{% block body %}

    <h3><span>Catalogue Delarue Teneze</h3>
    <table class="table table-striped">
    <thead>
    <tr>
        <th>Conte Type {{conteTypeCode }}</th>
        <th>Versions</th>
            {% for version in versions %}
            <tr>
            <td>

               <strong> 
                    {% if version.hasSource %} 
                        <a href="{{version.fichierSource }}">{{version.numero}}</a>
                    {% else %}
                        {{version.numero}}
                        
                    {% endif %}
                    - {{ version.reference }} 
                </strong></br>

                {% for ligne in version.description %}
                    {{ ligne }} </br>
                {% endfor %}
            </td>
        </tr>
    {% endfor %}

        
    </tr>
    </thead>
    <tbody>
    
    </tbody>



</table>
    


{% endblock %}